<?xml version="1.0" encoding="UTF-8"?>
<svg width="1920" height="400" viewBox="0 0 1920 400" xmlns="http://www.w3.org/2000/svg">
  <!-- 背景渐变 -->
  <defs>
    <linearGradient id="banner-bg" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#FFD3A5"/>
      <stop offset="100%" style="stop-color:#FD6585"/>
    </linearGradient>
  </defs>
  
  <!-- 背景 -->
  <rect width="1920" height="400" fill="url(#banner-bg)"/>
  
  <!-- 中药材元素组 -->
  <g transform="translate(250,200)">
    <!-- 滋补养生图标 -->
    <circle cx="0" cy="0" r="60" fill="none" stroke="#fff" stroke-width="4"/>
    <path d="M-30,-30 C-10,-10 10,-10 30,-30" stroke="#fff" stroke-width="4" fill="none"/>
    <path d="M-30,30 C-10,10 10,10 30,30" stroke="#fff" stroke-width="4" fill="none"/>
  </g>
  
  <!-- 文字 -->
  <text x="800" y="170" font-family="Arial" font-size="48" fill="#fff" font-weight="bold">
    滋补养生 调理身心
  </text>
  <text x="800" y="240" font-family="Arial" font-size="24" fill="#fff">
    四季调养 科学搭配
  </text>
  
  <!-- 装饰元素 -->
  <g transform="translate(1500,200)">
    <!-- 中药材图案 -->
    <path d="M0,-50 C20,-30 20,30 0,50" stroke="#fff" stroke-width="3" fill="none"/>
    <path d="M-20,-30 C0,-10 0,10 -20,30" stroke="#fff" stroke-width="3" fill="none"/>
    <path d="M20,-30 C40,-10 40,10 20,30" stroke="#fff" stroke-width="3" fill="none"/>
  </g>
  
  <!-- 点缀元素 -->
  <circle cx="1700" cy="100" r="30" fill="#fff" opacity="0.2"/>
  <circle cx="1750" cy="300" r="20" fill="#fff" opacity="0.2"/>
  <circle cx="1800" cy="200" r="25" fill="#fff" opacity="0.2"/>
</svg> 